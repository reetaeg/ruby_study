<p id="notice"><%= notice %></p>

<h1>Listing Trust Moneys</h1>

<table>
  <thead>
    <tr>
      <th>Source type</th>
      <th>Agency</th>
      <th>Etc</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trust_moneys.each do |trust_money| %>
      <tr>
        <td><%= trust_money.source_type %></td>
        <td><%= trust_money.agency %></td>
        <td><%= trust_money.etc %></td>
        <td><%= link_to 'Show', trust_money %></td>
        <td><%= link_to 'Edit', edit_trust_money_path(trust_money) %></td>
        <td><%= link_to 'Destroy', trust_money, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Trust money', new_trust_money_path %>
