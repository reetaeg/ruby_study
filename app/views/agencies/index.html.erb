<p id="notice"><%= notice %></p>

<h1>Listing Agencies</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Agency type</th>
      <th>Is used</th>
      <th>Area</th>
      <th>Edi code</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Owner name</th>
      <th>Biz number</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @agencies.each do |agency| %>
      <tr>
        <td><%= agency.name %></td>
        <td><%= agency.agency_type %></td>
        <td><%= agency.is_used %></td>
        <td><%= agency.area %></td>
        <td><%= agency.edi_code %></td>
        <td><%= agency.email %></td>
        <td><%= agency.phone %></td>
        <td><%= agency.owner_name %></td>
        <td><%= agency.biz_number %></td>
        <td><%= link_to 'Show', agency %></td>
        <td><%= link_to 'Edit', edit_agency_path(agency) %></td>
        <td><%= link_to 'Destroy', agency, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Agency', new_agency_path %>
